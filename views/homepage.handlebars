<!-- Loop over each object in the projects array -->
{{#each blogs as |blog| }}
<div class="row mb-4 blog">
  <div class="col-md-5">
    <h2>
      <!-- Call helper function with triple brackets to render the emoji span as HTML -->
      {{{get_emoji}}}
      <a href="/blogs/{{blogs.id}}">{{blogs.name}}</a>
    </h2>
    
    <p>Created by {{blogs.user.name}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{blogs.description}}
    </p>
  </div>
</div>
{{/each}}
